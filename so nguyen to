#include <iostream>
using namespace std;
int main()
{
    int n;
    int d, dem, tong;
    dem = 0;
    cin >> n;

    d = 0;
    if (n != 1)
    {
        for (int j = 2; j< int((n/2)); j++)
        {
            if (n % j == 0)
                d = d + 1;
        }
        if (d == 0)
        {
            cout << n << " " << ": la so nguyen to" << endl;
        }
        else cout << n << " " << ": khong phai la so nguyen to" << endl;
    }
    else cout << n << " " << ": khong phai la so nguyen to" << endl;



    return 0;
}
